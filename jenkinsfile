pipeline {
    agent any
    tools{
        maven 'Maven-frank-1'
    }
    
    stages{
        stage('Build'){
            steps {
                sh 'mvn clean package'
            }
            post {
                success {
                    echo '开始存档....'
                }
            }
        }

        stage('Unit Testing'){

            steps {
                echo "start SonarQubeEnv testing"

                    withSonarQubeEnv('franktest') {
                    sh '-Dproject.settings=custom_config/sonar-scanner.properties'
                }
            }
        }
    }
}


              