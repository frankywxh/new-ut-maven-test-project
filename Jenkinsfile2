pipeline {
    agent any
    tools{
        maven 'Maven-frank-1'
    }
    
    stages{
        

        stage('Deploy'){
            steps {
                sh 'docker rm -f websockerproject'
                sh 'docker pull registry.cn-hangzhou.aliyuncs.com/frankywxh/jenkins:v1'
                sh 'docker run --name websockerproject -p 8083:8080 -d registry.cn-hangzhou.aliyuncs.com/frankywxh/jenkins:v1'
            
            }

            post {
                success {
                    echo '正在构建'
                }
            }
        }

        stage('Jmeter') { 
            steps {
                sh "mkdir -p /var/jenkins_home/workspace/webhook-trigger/Jmeter/output"
                sh 'pwd'
                sh 'jmeter -n -t src/main/Jmeter/franktest.jmx  -l src/main/Jmeter/frank.jtl -e -o 123/report'
                step([$class: 'ArtifactArchiver', artifacts: 'src/main/Jmeter/*'])
                //sh 'pwd'
                //perfReport "src/main/Jmeter/frank.jtl"
            }
        }
    }
}


              