pipeline {
    agent any
    tools{
        maven 'Maven-frank-1'
    }
    
    stages{
        

        stage('Deploy'){
            steps {
                sh 'docker pull registry.cn-hangzhou.aliyuncs.com/frankywxh/jenkins:v1'
                sh 'docker run --name websockerproject -p 8083:8080 -d frankwebsocket'
            
            }

            post {
                success {
                    echo '正在构建'
                }
            }
        }
    }
}


              